@page "/Agenda"

<h3>Cadastro Agenda</h3>
<a href="InsertAgenda" class="btn btn-info">Nova Agenda</a>
@if (agendaView is null)
{
    <p>Carregando...</p>
}
else
{
    <TabelaAgenda agendaView="agendaView" />
}

@code {
    private AgendaView? agendaView;

    protected override Task OnInitializedAsync()
    {
        return base.OnInitializedAsync();
    }
}
